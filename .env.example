TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# File Path Access Password (optional, defaults to 'senha123' if not set)
FILE_PATH_PASSWORD=senha123

# Cloud Provider Selection (aws or runpod)
CLOUD_PROVIDER=aws

# AWS Credentials (required for cloud ML workflows)
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1

# AWS S3 Configuration
S3_BUCKET=ml-agent-data-YOUR_ACCOUNT_ID
S3_DATA_PREFIX=datasets
S3_MODELS_PREFIX=models
S3_RESULTS_PREFIX=results
S3_LIFECYCLE_DAYS=90

# AWS EC2 Configuration
EC2_INSTANCE_TYPE=m5.xlarge
EC2_SPOT_MAX_PRICE=0.5
EC2_AMI_ID=your_deep_learning_ami_id_here
EC2_KEY_NAME=ml-agent-key
EC2_SECURITY_GROUP=your_security_group_id_here

# AWS Lambda Configuration
LAMBDA_FUNCTION_NAME=ml-agent-prediction
LAMBDA_MEMORY_MB=3008
LAMBDA_TIMEOUT_SECONDS=900
LAMBDA_LAYER_ARN=your_lambda_layer_arn_here

# Cost Limits
MAX_TRAINING_COST_DOLLARS=10.0
MAX_PREDICTION_COST_DOLLARS=1.0
COST_WARNING_THRESHOLD=0.8

# Security (Optional)
KMS_KEY_ID=your_kms_key_id_here
IAM_ROLE_ARN=your_iam_role_arn_here

# RunPod Credentials (required when CLOUD_PROVIDER=runpod)
RUNPOD_API_KEY=your_runpod_api_key_here
RUNPOD_NETWORK_VOLUME_ID=your_network_volume_id_here
RUNPOD_STORAGE_ACCESS_KEY=your_storage_access_key_here
RUNPOD_STORAGE_SECRET_KEY=your_storage_secret_key_here

# RunPod Configuration
RUNPOD_STORAGE_ENDPOINT=https://storage.runpod.io
RUNPOD_DEFAULT_GPU_TYPE=NVIDIA RTX A5000
RUNPOD_CLOUD_TYPE=COMMUNITY
RUNPOD_DATA_PREFIX=datasets
RUNPOD_MODELS_PREFIX=models
RUNPOD_RESULTS_PREFIX=results

# RunPod Serverless (set after creating endpoint)
RUNPOD_ENDPOINT_ID=your_endpoint_id_here

# RunPod Docker Registry (optional)
RUNPOD_DOCKER_REGISTRY=your_docker_username